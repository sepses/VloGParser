{
  "title": "Host/Agent Log Parser server",
  "url": "http://localhost",
  "port": 8080,
  "logSources": [ 
     {
	      "title": "authlog",
	      "type": "File",
	      "logLocation": "experiment/logfile/auth.log",
	      "mapping": "experiment/rml/authlog.rml",
	      "grokFile": "experiment/pattern/pattern.grok",
	      "grokPattern": "%{SYSLOGBASE} %{GREEDYDATA:message}",
	      "outputModel": "experiment/output/authlog_model.ttl",
	      "namegraph": "http://w3id.org/sepses/graph/log/authlog",
	      "regexMeta": "experiment/model/regexMeta.ttl",
	      "regexOntology": "experiment/model/regexOntology.ttl",
	      "vocabulary":"https://w3id.org/sepses/vocab/log/authLog#",
	      "logDateFormat":"MMM d HH:mm:ss",
	      "logTimeRegex":"\\w+\\s+\\d+\\s\\d{2}:\\d{2}:\\d{2}"      
    	},
      {
	      "title": "apachelog",
	      "type": "File",
	      "logLocation": "experiment/logfile/apache.log",
	      "mapping": "experiment/rml/apachelog.rml",
	      "grokFile": "experiment/pattern/pattern.grok",
	      "grokPattern": "%{COMMONAPACHELOG}",
	      "outputModel": "experiment/output/apache_model.ttl",
	      "namegraph": "http://w3id.org/sepses/graph/log/apachelog",
	      "regexMeta": "experiment/model/regexMeta.ttl",
	      "regexOntology": "experiment/model/regexOntology.ttl",
	      "vocabulary":"https://w3id.org/sepses/vocab/log/apacheLog#",
	      "logDateFormat":"MMM d HH:mm:ss",
	      "logTimeRegex":"\\w+\\s+\\d+\\s\\d{2}:\\d{2}:\\d{2}"       
     },{
	      "title": "snortAlertlog",
	      "type": "File",
	      "logLocation": "experiment/logfile/snort-alert-fast.log",
	      "mapping": "experiment/rml/snortAlertlog.rml",
	      "grokFile": "experiment/pattern/pattern.grok",
	      "grokPattern": "%{SNORT}",
	      "outputModel": "experiment/output/snort_alert_model.ttl",
	      "namegraph": "http://w3id.org/sepses/graph/log/snortAlertlog",
	      "regexMeta": "experiment/model/regexMeta.ttl",
	      "regexOntology": "experiment/model/regexOntology.ttl",
	      "vocabulary":"https://w3id.org/sepses/vocab/log/snort-alert#",
	      "logDateFormat":"MM/dd-HH:mm:ss",
	      "logTimeRegex":"\\d{2}\/\\d{2}-\\d{2}:\\d{2}:\\d+"      
     }
    ],
    "endpoint": {
	    "sparqlEndpoint":   "http://localhost:8890/sparql",
	    "user":  "dba",
	    "pass":   "dba"
    }

  }
