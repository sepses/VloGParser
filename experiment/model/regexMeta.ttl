@prefix : <http://w3id.org/sepses/resource/regexMeta#> .
@prefix auth: <https://w3id.org/sepses/vocab/log/authLog#> .
@prefix cl: <https://w3id.org/sepses/vocab/log/coreLog#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix regex: <http://w3id.org/sepses/vocab/ref/regex#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://w3id.org/sepses/vocab/log/authLog> a owl:Ontology ;
    regex:hasGrokPattern "%{SYSLOGBASE} %{GREEDYDATA:message}" .

<http://w3id.org/sepses/vocab/log/coreLog> a owl:Ontology ;
    regex:hasGrokPattern "%{SYSLOGBASE} %{GREEDYDATA:message}" .

auth:userName a rdf:Property ;
    regex:hasRegexPattern regex:user .

cl:originatesFrom a rdf:Property ;
    regex:hasRegexPattern regex:host ;
    rdfs:label "logsource" .

cl:hostName a rdf:Property ;
    regex:hasRegexPattern regex:host ;
    rdfs:label "logsource" .

auth:programName a rdf:Property ;
    rdfs:label "program" .

auth:pid a rdf:Property ;
    rdfs:label "pid" .

cl:ipAddress a rdf:Property ;
    regex:hasRegexPattern regex:ip4Address,
        regex:ip6Address .

cl:timestamp a rdf:Property ;
    regex:hasRegexPattern regex:timestamp .